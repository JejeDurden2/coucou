{
  "name": "@coucou-ia/api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nest start --watch",
    "build": "pnpm --filter @coucou-ia/shared build && prisma generate && nest build",
    "start": "nest start",
    "start:prod": "prisma migrate deploy && node -r ./dist/tracing dist/main",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "sentiment:trigger": "tsx scripts/trigger-sentiment-scan.ts",
    "update:disposable-domains": "tsx scripts/update-disposable-domains.ts"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.74.0",
