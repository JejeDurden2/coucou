    "date-fns": "^4.1.0",
    "dotenv": "^17.2.4",
    "helmet": "^8.1.0",
    "ioredis": "^5.9.2",
    "neverthrow": "^8.2.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
